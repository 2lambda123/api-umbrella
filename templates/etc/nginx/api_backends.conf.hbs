{{#each apis}}
  # {{name}}
  upstream api_umbrella_{{_id}}_backend {
    {{#unless defaultBalance}}
      {{balance_algorithm}};
    {{/unless}}

    {{#if keepalive_connections}}
      keepalive {{keepalive_connections}};
    {{/if}}

    {{#each servers}}
      dynamic_server {{host}}:{{port}};
    {{/each}}
  }
{{/each}}
