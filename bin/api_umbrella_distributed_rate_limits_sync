#!/usr/bin/env node

var DistributedRateLimitsSync = require('../lib/distributed_rate_limits_sync').DistributedRateLimitsSync;
    nomnom = require('nomnom');

var options = nomnom
  .option('config', {
    abbr: 'c',
    metavar: 'FILE',
    default: ['/etc/api-umbrella/api-umbrella.yml'],
    list: true,
    help: 'Conifg file to use (include multiple times for multiple files)',
  })
  .parse();

new DistributedRateLimitsSync();
