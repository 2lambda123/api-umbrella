{
  "mongodb": "mongodb://127.0.0.1:27017/api_umbrella_development",
  "redis": {
    "host": "127.0.0.1",
    "port": 6379
  },
  "account_signup_uri": "http://example.com/",
  "contact_uri": "http://example.com/contact",
  "trusted_proxies": ["127.0.0.1"],
  "proxy": {
    "workers": 1,
    "host": "0.0.0.0",
    "port": 7890,
    "target": "127.0.0.1:50100",
    "api_key_methods": [
      "header",
      "get_param",
      "basic_auth_username"
    ],
    "rate_limits": [
      {
        "duration": 1000,
        "accuracy": 500,
        "limit_by": "ip",
        "limit": 250000,
        "distributed": false
      }, {
        "duration": 1000,
        "accuracy": 500,
        "limit_by": "api_key",
        "limit": 150000,
        "distributed": false
      }, {
        "duration": 3600000,
        "accuracy": 60000,
        "limit_by": "api_key",
        "limit": 10000000,
        "distributed": true
      }
    ],
    "restricted_apis": []
  },
  "log_processor": {
    "workers": 1
  },
  "haproxy_log_listener": {
    "workers": 1,
    "port": 5114
  }
}
