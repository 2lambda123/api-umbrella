# GitHub Pages Backend
#
# The static site that provides the frontend pages and documenation.
backend github_pages_farm
  acl url_match_old_doc path_reg ^/doc($|/)
  redirect location /docs/ if url_match_old_doc

  # Modify the host.
  reqirep ^Host: Host:\ api.data.gov

  balance roundrobin
  server github_pages1 nrel.github.io:80
