backend api_bjs_farm
  # Strip /bjs from the beginning of the request URL before the receiving
  # backend application sees the request.
  #
  # Turns a request from "GET /bjs/something" to "GET /something"
  reqirep ^([^\ ]*)\ /bjs(/.*) \1\ \2

  # Modify the host.
  reqirep ^Host: Host:\ bjs.gov:8080

  balance roundrobin
  server api_bjs1 bjs.gov:8080
