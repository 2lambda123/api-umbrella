# Authentication Backend
#
# These backend proxy servers authenticate all the /api requests.
backend <%= subdomain_prefix %>auth_proxy_farm
  balance roundrobin
  <% server_registory[:auth_proxy].each_with_index do |server, i| -%>
  server <%= subdomain_prefix %>auth_proxy<%= i %> <%= server[:host] %>:<%= server[:port] %>
  <% end -%>
