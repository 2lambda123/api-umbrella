backend api_myusa_discovery_farm
  # Strip /myusa/discovery from the beginning of the request URL before the receiving
  # backend application sees the request.
  #
  # Turns a request from "GET /myusa/discovery/something" to "GET /something"
  reqirep ^([^\ ]*)\ /myusa/discovery(/.*) \1\ \2

  # Modify the host.
  reqirep ^Host: Host:\ discovery.my.usa.gov

  balance roundrobin
  server api_broadbandmap1 discovery.my.usa.gov:80
