# Ignore bundler config
/.bundle

# Ignore local bundle
/vendor/bundle

# Ignore local npm modules
/node_modules

# Ignore Capistrano files
/REVISION
/config/deployed.yml

# Ignore files generated from templates
/config/elasticsearch/elasticsearch-env.sh
/config/elasticsearch/elasticsearch.yml
/config/mongod.conf
/config/nginx/api_backends.conf
/config/nginx/public_defaults.conf
/config/nginx/router.conf
/config/nginx/site.conf
/config/nginx/web.conf
/config/nginx/ssl_terminator.conf
/config/redis.conf
/config/supervisor/gatekeeper.conf
/config/supervisord.conf

# Ignore node generated file
/config/gatekeeper/nginx.conf
/config/gatekeeper/runtime.json
/config/gatekeeper/runtime.yml*
/test/config/runtime.yml
/test/config/nginx.conf

# Ignore log files
/log/*
/log/gatekeeper/*.log
